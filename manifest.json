{
	"manifest_version": 3,
	"name": "CraftingChrome",
	"description": "Cooking Mama -themed Pomodoro timer.",
	"version": "1.0",
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "assets/icon.png"
		}
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"activeTab",
		"alarms",
		"notifications",
		"storage",
		"tabs",
		"scripting"
	],
	"icons": {
		"128": "assets/icon.png"
	}
}
